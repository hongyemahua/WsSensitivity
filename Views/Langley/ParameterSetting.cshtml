<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>修改分析参数</title>
    <meta name="renderer" content="webkit">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=0">
    <link href="~/Static/layui/layui/css/layui.css" rel="stylesheet" />
</head>
@{ 
    int ds = (int)ViewData["ds"];
    int correction = (int)ViewData["correction"];
}
<body>

    <div class="layui-form" lay-filter="form-addrole" id="form-addrole" style="padding: 20px 30px 0 0;">
        <div class="layui-form-item">
            <div class="layui-input-block">
                @if (ds == 0)
                {
                    <input type="radio" name="let_DistributionState" value="0" title="正态分布" checked>
                    <input type="radio" name="let_DistributionState" value="1" title="逻辑斯蒂分布">
                }
                else
                {
                    <input type="radio" name="let_DistributionState" value="0" title="正态分布">
                    <input type="radio" name="let_DistributionState" value="1" title="逻辑斯蒂分布" checked> 
                }

            </div>
        </div>
        <div class="layui-form-item">
            <div class="layui-input-block">
                @if (correction == 0)
                {
                    <input type="radio" name="let_Correction" value="0" title="标准差修正" checked>
                    <input type="radio" name="let_Correction" value="1" title="标准差不修正">
                }
                else
                {
                    <input type="radio" name="let_Correction" value="0" title="标准差修正" >
                    <input type="radio" name="let_Correction" value="1" title="标准差不修正" checked>
                }

            </div>
        </div>
        <div class="layui-form-item layui-hide">
            <button class="layui-btn" lay-submit lay-filter="role_submit" id="role_submit">提交</button>
        </div>
    </div>

    <script src="~/Static/layui/layui/layui.js"></script>
    <script>
        layui.config({
            base: '../static/layui/' //静态资源所在路径
        }).extend({
            index: 'lib/index' //主入口模块
        }).use(['index', 'form', 'role_validation'], function () {
            var $ = layui.$
                , form = layui.form;
        })
    </script>
</body>
</html>
