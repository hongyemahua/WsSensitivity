<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>添加角色</title>
    <meta name="renderer" content="webkit">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=0">
    <link href="~/Static/layui/layui/css/layui.css" rel="stylesheet" />
</head>
<body>

    <div class="layui-form" lay-filter="form-addrole" id="form-addrole" style="padding: 20px 30px 0 0;">
        <div class="layui-form-item">
            <div class="layui-input-block">
                <input type="radio" name="分布" value="正态分布" title="正态分布" checked>
                <input type="radio" name="分布" value="逻辑斯蒂分布" title="逻辑斯蒂分布">
            </div>
        </div>
        <div class="layui-form-item">
            <div class="layui-input-block">
                <input type="radio" name="修正" value="标准参修正" title="标准参修正" checked>
                <input type="radio" name="修正" value="标准参不修正" title="标准参不修正">
            </div>
        </div>
        <div class="layui-form-item layui-hide">
            <button class="layui-btn" lay-submit lay-filter="role_submit" id="role_submit">提交</button>
        </div>
    </div>

    <script src="~/Static/layui/layui/layui.js"></script>
    <script>
        layui.config({
            base: '../static/layui/' //静态资源所在路径
        }).extend({
            index: 'lib/index' //主入口模块
        }).use(['index', 'form', 'role_validation'], function () {
            var $ = layui.$
                , form = layui.form;
        })
    </script>
</body>
</html>
