        layui.config({
            base: '../static/layui/' //静态资源所在路径
        }).extend({
            index: 'lib/index' //主入口模块
        }).use(['index', 'form', 'role_validation'], function () {
            var $ = layui.$
                , form = layui.form;

            @*var active = {

                nocyte: function (layero) {

                        $.ajax({
                            type: "post",
                            url: '@Url.Action("LanglieParameterSettingsJson")',
                            datatype: "json",
                            contenttype: "application/json; charset=utf-8",
