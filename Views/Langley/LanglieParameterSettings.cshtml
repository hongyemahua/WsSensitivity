
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>兰利法参数设置</title>
    <meta name="renderer" content="webkit">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=0">
    <link href="~/static/layui/layui/css/layui.css" rel="stylesheet" />
    <link href="~/static/layui/style/admin.css" rel="stylesheet" />

</head>
<body>
    <div class="layui-fluid">
        <div class="layui-row">
            <div class="layui-col-sm12">
                @*<div class="layui-card layui-col-sm3">
                </div>*@
                <div class="layui-card layui-col-sm12">
                    <div class="layui-card-body">
                        @*<form class="layui-form layui-form-pane" lay-filter="layuiadmin-form-useradmin" id="layuiadmin-form-useradmin" action="../Langley/LanglieParameterSettings" method="post">*@
                        <div class="layui-form layui-form-pane" lay-filter="layuiadmin-form-useradmin" id="layuiadmin-form-useradmin">
                            <div class="layui-fluid">
                                <div class="layui-row layui-col-space4">
                                    <div class="layui-col-md12">
                                        <div class="layui-card">
                                            <div class="layui-card-body layui-row layui-col-space10">

                                                <div class="layui-col-md12 layui-col-space10">

                                                    <div class="layui-col-md12 layui-col-space5">
                                                        <div class="layui-col-md6">
                                                            <label class="layui-form-label">产品名称</label>
                                                            <div class="layui-input-block">
                                                                <input type="text" name="let_ProductName" id="productname"  lay-verify="required" placeholder="新产品" autocomplete="off" class="layui-input" value="新产品">
                                                            </div>
                                                        </div>
                                                        <div class="layui-col-md6">
                                                            <label class="layui-form-label">仪器分辨率</label>
                                                            <div class="layui-input-block">
                                                                <input type="text" name="let_PrecisionInstruments" id="Instrument" lay-verify="required|number" placeholder="0.001" value="0.001" autocomplete="off" class="layui-input">
                                                            </div>
                                                        </div>
                                                    </div>

                                                    <div class="layui-col-md12 layui-col-space5">
                                                        <div class="layui-col-md6">
                                                            <label class="layui-form-label">刺激量下限</label>
                                                            <div class="layui-input-block">
                                                                <input type="text" name="let_StimulusQuantityFloor" id="let_StimulusQuantityFloor" lay-verify="required|number" placeholder="0" value="0" autocomplete="off" class="layui-input">
                                                            </div>
                                                        </div>
                                                        <div class="layui-col-md6">
                                                            <label class="layui-form-label">刺激量上限</label>
                                                            <div class="layui-input-block">
                                                                <input type="text" name="let_StimulusQuantityCeiling" id="let_StimulusQuantityCeiling" lay-verify="required|number" placeholder="100" value="100" autocomplete="off" class="layui-input">
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="layui-card layui-col-sm6">
                                        <div class="layui-card-body">
                                            <fieldset class="layui-elem-field">
                                                <div class="layui-field-box">
                                                    <div class="layui-form layui-form-pane" lay-filter="layuiadmin-form-useradmin" id="layuiadmin-form-useradmin">
                                                        <div class="layui-fluid">
                                                            <div class="layui-row layui-col-space3">
                                                                <div class="layui-card">
                                                                    <div class="layui-col-md12 layui-col-space10">
                                                                        <div class="layui-col-md12 layui-col-space5">
                                                                            <input type="radio" name="let_DistributionState" value="0" title="正态分布" checked>
                                                                            <input type="radio" name="let_DistributionState" value="1" title="逻辑斯谛分布">
                                                                        </div>
                                                                    </div>

                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </fieldset>
                                            <fieldset class="layui-elem-field">
                                                <div class="layui-field-box">
                                                    <div class="layui-form layui-form-pane" lay-filter="layuiadmin-form-useradmin">
                                                        <div class="layui-fluid">
                                                            <div class="layui-row layui-col-space3">
                                                                <div class="layui-card">
                                                                    <div class="layui-col-md12 layui-col-space10">
                                                                        <div class="layui-col-md12 layui-col-space3">
                                                                            <input type="radio" name="let_Correction" value="1" title="标准差不修正">
                                                                            <input type="radio" name="let_Correction" value="0" title="标准差修正" checked>
                                                                        </div>
                                                                        
                                                                    </div>

                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </fieldset>

                                        </div>
                                    </div>

                                    <div class="layui-card layui-col-sm6">
                                        <div class="layui-card-body">
                                            <fieldset class="layui-elem-field" style="height:180px">
                                                <div class="layui-field-box">
                                                    <div class="layui-form layui-form-pane" lay-filter="layuiadmin-form-useradmin" id="layuiadmin-form-useradmin">
                                                        <div class="layui-fluid">
                                                            <div class="layui-row layui-col-space15">
                                                                <div class="layui-card">
                                                                    <div class="layui-col-md12 layui-col-space15">
                                                                        <div class="layui-col-md12 layui-col-space15">
                                                                            <input type="radio" name="let_StandardState" value="0" title="标准" checked>
                                                                            <input type="radio" name="let_StandardState" value="1" title="对数ln">
                                                                            <input type="radio" name="let_StandardState" value="2" title="对数log">
                                                                        </div>
                                                                        <div class="layui-col-md12 layui-col-space15">
                                                                            <input type="radio" name="let_StandardState" value="3" title="幂">
                                                                            = <input type="text" name="let_Power" placeholder="0" id="getPow" lay-verify="required|number" class="layui-input-inline" value="0">
                                                                        </div>
                                                                        <div class="layui-col-md12 layui-col-space3 layui-input-block ">
                                                                            <input type="checkbox" name="let_FlipTheResponse" title="翻转响应" value="1">
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </fieldset>

                                        </div>
                                    </div>
                                    <div class="layui-card layui-col-sm12">
                                        <div class="layui-card-body">
                                            <fieldset class="layui-elem-field">
                                                <div class="layui-form-item layui-form-text">
                                                    <label class="layui-form-label">技术条件</label>
                                                    <div class="layui-input-block">
                                                        <textarea placeholder="请输入......" class="layui-textarea" name="let_TechnicalConditions"></textarea>
                                                    </div>
                                                </div>
                                            </fieldset>

                                        </div>
                                    </div>
                                    <div class="layui-card layui-col-md12 layui-hide">
                                        <div class="layui-card-body layui-row layui-col-space10">
                                            <div class="layui-col-md12 layui-col-space10">
                                                <div class="layui-col-md6">
                                                    <button type="submit" class="layui-btn" style="width:100%;height:200%;" lay-submit lay-filter="submit_Btn" id="submit_Btn">确定</button>
                                                </div>
                                                <div class="layui-col-md6">
                                                    <button type="reset" class="layui-btn layui-btn-primary" id="reset_Btn" style="width:100%;">重置</button>
                                                </div>
                                            </div>

                                        </div>
                                    </div>

                                </div>
                            </div>
                        </div>
                        @*</form>*@
                    </div>
                </div>
            </div>
        </div>

    </div>
    <script src="~/Static/layui/layui/layui.js"></script>
