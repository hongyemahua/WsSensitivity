                                            <div class="layui-input-block">
                                                <label class="layui-form-label" style="width:5%" id="label3">0</label>
                                            </div>
                                        </div>
                                    </div>
                                    
                                </div>
                            </div>
                        </div>
                    </div>
                    <!--单组实验结果-->
